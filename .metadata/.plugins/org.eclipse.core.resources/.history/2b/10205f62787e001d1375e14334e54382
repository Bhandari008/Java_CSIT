package MultiThreading;

class Good{
	void display() throws InterruptedException {
		for(int i=0;i<10;i++)
		{
			Thread.sleep(10000);
			System.out.println("Good");
		}
	}
}

class Students{
	void display() {
		for(int i=0;i<10;i++)
		{
			System.out.println("Students");
		}
	}
}


public class WithoutMultiThreading {

	public static void main(String[] args) {
	

	}

}
